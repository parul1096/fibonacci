    "def sum_of_list(l,n):\n",
    "  if n == 0:\n",
    "    return l[n];\n",
    "  return l[n] + sum_of_list(l,n-1)\n",
    "\n",
    "my_list = [1,3,5,2,4,6,8,9]\n",
    "print (\"The sum of my_list is\", sum_of_list(my_list,len(my_list)-1))"
   ]
  }
