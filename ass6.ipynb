    "class Dog:\n",
    "    def __init__(self,name,age,coat_color):\n",
    "        self.name = name\n",
    "        self.age = age\n",
    "        self.coat_color = coat_color\n",
    "    \n",
    "    def description(self):\n",
    "        print(f'Hi, my name is {self.name} and I am {self.age} years old!')\n",
    "    def get_info(self):\n",
    "        print (f'The color of my coat is {self.coat_color}')\n",
    "\n",
    "class JackRussellTerrier(Dog):\n",
    "        def __init__ (self,times,fur_size):\n",
    "            self.times = times\n",
    "            self.fur_size = fur_size\n",
    "        def speak(self):\n",
    "            print (f'{self.name} says:woof!{self.times}')\n",
    "        def furry(self):\n",
    "            print('{self.name} is {self.fur_size} furred dog.')\n",
    "\n",
    "class Bulldog(Dog):\n",
    "        def __init__ (self,times,fur_size):\n",
    "            self.times = times\n",
    "            self.fur_size = fur_size\n",
    "        def speak(self):\n",
    "            print (f'{self.name} says:woof!{self.times}')\n",
    "        def furry(self):\n",
    "            print('{self.name} is {self.fur_size} furred dog.')\n",
    "\n",
    "d1 = Dog('Naughty',2,'white')\n",
    "d2 = Dog('luko',1,'brown')\n",
    "print (d1.description(),d1.get_info())\n",
    "print (d2.description(),d2.get_info())"
   ]
